/**
  * @brief  DCMI error callback.
  * @param  hdcmi: pointer to DCMI_HandleTypeDef structure.
  * @retval None
  */
void HAL_DCMI_ErrorCallback(DCMI_HandleTypeDef *hdcmi)
{
  if (hdcmi->ErrorCode & HAL_DCMI_ERROR_SYNC)
  {
    // Set our flag if we get a synchronization error
    dcmi_sync_error_detected = 1;
  }
}

/**
  * @brief  DCMI frame event callback.
  * @param  hdcmi: pointer to DCMI_HandleTypeDef structure.
  * @retval None
  */
void HAL_DCMI_FrameEventCallback(DCMI_HandleTypeDef *hdcmi)
{
  // Set our flag when a frame is successfully captured
  dcmi_frame_event_detected = 1;
}

/* USER CODE END 4 */

